{
  "manifest_version": 2,
  "name": "TETR.IO config saver",
  "version": "1.5",

  "description": "",
  "page_action": {
    "show_matches": ["*://*.tetr.io/"],
    "default_title": "Save config",
    "default_popup": "./index.html",
    "default_icon": {
      "19": "./icons/tetrio-color.svg",
      "38": "./icons/tetrio-color.svg"
    }
  },
  "permissions": [
    "activeTab",
    "storage",
    "*://*.tetr.io/"
  ]

}
